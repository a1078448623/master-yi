<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wuyan.masteryi.mall.mapper.SkGoodsMapper">
    <select id="getPrice" resultType="float">
       select newprice
       from skgoods where goods_id=#{goodsId}
    </select>

    <select id="getAllSk" resultType="SkGoods">
        select * from skgoods
    </select>

</mapper>
