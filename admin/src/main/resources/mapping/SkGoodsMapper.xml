<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wuyan.masteryi.admin.mapper.SkGoodsMapper">
    <insert id="setSkGoods">
        insert into skgoods value(#{goodsId},#{goodsName},#{goodsCoverUrl},#{stock},#{newprice},#{oldprice},#{bDate},#{eDate})
    </insert>

    <delete id="deleteSkGoods">
        delete from skgoods where goodsId = #{goodsId}
    </delete>


</mapper>
